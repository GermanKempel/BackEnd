<html>
  <head>
    <title>Products</title>
  </head>
  <body>
    <h1>Product List</h1>
    <ul>
      {{#each products.docs}}
        <li>
          <h2>{{title}}</h2>
          <p>{{description}}</p>
          <p>Price: ${{price}}</p>
          <p>Stock: {{stock}}</p>
        </li>
      {{/each}}
    </ul>
  </body>
</html>